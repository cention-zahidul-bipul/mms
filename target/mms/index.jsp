	<body>

	
	
	<div id="container">
		<div id="header"></div>
		<div id="content">
			<span>Login</span>
			<form action="login" method="post">
				<table border="0" align="center">
					<tr>
						<td>User Name:</td>
						<td><input type="text" name="uname"></td>
					</tr>
					<tr>
						<td>Password:</td>
						<td><input type="password" name="password"></td>
					</tr>
					<tr>
						<td colspan="2" align="right"><input type="submit" value="login"> &nbsp; or &nbsp;
						<a href="registration.jsp">Create account</a></td>
					</tr>
				</table>
			</form>
		</div>
